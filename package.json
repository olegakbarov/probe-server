{
  "name": "probe-server",
  "version": "1.0.0",
  "description": "Real-time matchmaking server",
  "main": "server.js",
  "scripts": {
    "test-watch": "ava test --watch | faucet",
    "test": "ava test | faucet",
    "build": "webpack -p --config webpack.config.js",
    "babel-node": "babel-node",
    "dev": "nodemon --exec npm run babel-node server.js",
    "production": "pm2 start ./dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:olegakbarov/probe-server.git"
  },
  "author": "@olegakbarov",
  "license": "CLOSEDSOUCE!",
  "dependencies": {
    "ava": "^0.13.0",
    "axios": "^0.9.1",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-node5": "^11.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "bunyan": "^1.8.0",
    "bunyan-prettystream": "^0.1.3",
    "eventsource": "^0.2.1",
    "install": "^0.6.1",
    "npm": "^3.8.6",
    "tap-dot": "^1.0.4",
    "webpack": "^1.12.15"
  },
  "ava": {
    "files": [
      "test/*.js",
      "!**/not-this-file.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "match": [],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
